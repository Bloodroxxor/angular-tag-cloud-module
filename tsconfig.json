{
  "version": "2.0.3",
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
  "angularCompilerOptions": {
    "genDir": "compiled"
  }
}
